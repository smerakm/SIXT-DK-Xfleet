<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Regulated_tax_at_re_calculation__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
 Period__c &gt; 4,
 Total_registration_tax__c * 0.06 / 100 * Period__c,
0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>IF Period__c &gt; 4 THEN Total_registration_tax__c * 0.06 / 100 * Period__c</inlineHelpText>
    <label>Regulated tax at re-calculation</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>

    <decisions>
        <description>De-Qualifying criteria – vehicle should be moved to status SOLD and defleet status =”” when
COC document = Sent to buyer
Registration document =”Sent to buyer”
Key sent to buyer = True</description>
        <name>Defleet_Process_Completed</name>
        <label>Defleet Process Completed?</label>
        <locationX>182</locationX>
        <locationY>2687</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_defleet_completed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.COC_document__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sent to Buyer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Registration_document__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sent to buyer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Key_sent_to_buyer__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Vehicle_status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>00-</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Re_marketing_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sold</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Exit_Defleet_Process_and_set_to_SOLD</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>Yes</label>
        </rules>
    </decisions>
